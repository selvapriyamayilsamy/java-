import java.util.*;
public class main{
    public static void main (string[]args)
    {
        Scanner s = new Scanner(System.in);
        int a = s.nextInt();
        int b = s.nextInt();
        int c = ((a+b) + math.abs (a-b))/2
        System.out.println("greater"+c)
        int d = ((a+b) - math.abs (a-b))/2
        System.out.println("greater"+c)
    }
}