import java.util.*;
public class Main {
    public static void main(String[] args) {
        Scanner s = new Scanner(System.in);
        int m = s.nextInt();
        int n = s.nextInt();
        int x = s.nextInt();
        if( m % 2 == 0 )
        {
            m=m+1;
        }
        for(int i = m , count = 0 ; i <= n && count < x ;i=i+2)
        {

            System.out.println(i);
            count++;
        }
    }
}