import java.util.*;
public class Main {
    public static void main(String[] args) {
        Scanner s = new Scanner(System.in);
        int m = s.nextInt();
        int n = s.nextInt();
        int power = 1;
        for(int i = 1 ; i <= n ; i++)
        {
            power *= m;

        }
        System.out.println(power);
    }
}