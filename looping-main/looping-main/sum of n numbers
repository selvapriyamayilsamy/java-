import java.util.*;
public class Main {
    public static void main(String[] args) {
        Scanner s = new Scanner(System.in);
        int m = s.nextInt();
        int n = s.nextInt();
        int sum = 0;
        for(int i = m ; i <= n ; i++)
        {
            sum = sum + i ;
        }
        System.out.println(sum);
    }
}