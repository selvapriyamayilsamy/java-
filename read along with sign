import java.util.*;
public class main{
    public static void main (string[]args)
    {
        Scanner s = new Scanner(System.in);
        int n = s.nextInt();
        if(n>0)
        System.out.printf("%+d",n);
        else if (n<0)
        System.out.printf("%-d",n);
        else
        System.out.println("negative");
        
    }
}